<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.murali.outofboxforstudent"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.murali.outofboxforstudent.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
			android:label="@string/app_name"
			android:name=".AboutMe" >
            
            <intent-filter >
			<action android:name="com.murali.outofboxforstudent.AboutMe" />
			
			<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>

		</activity>
		
        <activity
			android:label="@string/app_name"
			android:name=".Comments" >
            
            <intent-filter >
			<action android:name="com.murali.outofboxforstudent.Comments" />
			
			<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>

		</activity>
    </application>

</manifest>
